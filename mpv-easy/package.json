  "description": "@mpv-easy/easy",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "bundle": "esbuild ./src/mpv-easy.tsx --bundle --outdir=dist --charset=utf8",
    "es5": "babel --presets  @babel/preset-env ./dist -d es5 --plugins ./src/babel/hack.js",
    "dev": "chokidar \"../mpv-*/src/**/*.(ts|tsx|json)\" -c \"pnpm run build && pnpm run copy\" -t 3000  --initial",
    "dev-ts": "tsc -p ./tsconfig.build.json -w",
    "echo": "chokidar \"./\" -c \"echo hello\" -d 0 -t 0 --initial",
    "build": "pnpm run bundle && pnpm run es5",
    "copy": "cp -r ./es5/mpv-easy.js $MPV_SCRIPT_DIR/mpv-easy.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel": "minify ./dist/mpv-easy.js --out-file ./dist/input.min.js"
  },
