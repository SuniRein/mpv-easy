{
  "name": "mpv-easy",
  "version": "0.1.10",
  "files": ["dist", "readme.md", "package.json"],
  "description": "TS and React toolkit for mpv script",
  "scripts": {
    "case-police": "case-police \"**/*.ts\"  fix",
    "dev": "pnpm run --parallel dev",
    "es5": "pnpm run --parallel es5",
    "build": "pnpm run -r build",
    "example": "pnpm run --parallel dev",
    "lint": "biome check --apply-unsafe  ./",
    "format": "biome format --write  ./",
    "lint:fix": "biome lint ./ --write --unsafe",
    "test": "pnpm run --parallel test",
    "clear": "rm -rf node_modules && pnpm -r exec rm -rf node_modules dist es5",
    "clear-dist": "rm -rf dist es5 && pnpm -r exec rm -rf dist es5",
    "release": "pnpm publish -r --access public",
    "release-alpha": "pnpm publish -r --access public --tag alpha"
  },
  "keywords": ["TS", "React", "mpv", "mpv script"],
  "author": "ahaoboy",
  "license": "MIT",
  "devDependencies": {
    "js-base64": "^3.7.7",
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@biomejs/biome": "^1.8.3",
    "@chokidar-napi/chokidar": "0.1.3",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-reconciler": "^0.28.8",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-minify": "^0.5.2",
    "case-police": "^0.6.1",
    "core-js": "^3.37.1",
    "dotenv": "^16.4.5",
    "esbuild": "^0.19.12",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-reconciler": "^0.29.2",
    "react-redux": "^9.1.2",
    "react-use": "^17.5.0",
    "redux": "^5.0.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
